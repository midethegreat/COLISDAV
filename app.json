{
  "expo": {
    "name": "CampusRide Nigeria",
    "slug": "campusride-nigeria",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/colisdav.png",
    "scheme": "cid",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.rork.campusride-nigeria"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/colisdav.png",
        "backgroundColor": "#00000000"
      },
      "package": "app.rork.campusride_nigeria",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "cid.dev",
              "pathPrefix": "/wallet"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://cid.dev"
      },
      "eas": {
        "projectId": "3f0a8c23-5645-4303-b3ff-dc5f6382cfee"
      }
    }
  }
}
